.senat-obvod
  position relative
  width 100%
  .kandidati
    float left
    clear left
    width 100%
    margin-top 14px
    .kandidat
      color #333
      .name
        font-weight bold
      .procent
        font-weight bold
      &:last-child
        .delim
          display none
    .kandidat-0
    .kandidat-1
      color black
      float left
      display block
      width 50%
      font-size 20px
      margin-bottom 8px
      position relative
      padding-left 30px
      .strana-kost
        top -1px
        left 0px
        display block
        position absolute
        width 20px
        height 20px
        border-radius 2px
      .delim
        display none
    .kandidat-1
      font-size 18px
  .supplemental
    top 0px
